<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.atlassian.theplugin.idea.GeneralConfigForm">
  <grid id="27dc6" binding="mainPanel" layout-manager="FormLayout">
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:d:grow"/>
    <rowspec value="top:3dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <colspec value="fill:d:grow"/>
    <constraints>
      <xy x="20" y="20" width="567" height="400"/>
    </constraints>
    <properties/>
    <border type="empty">
      <size top="12" left="12" bottom="12" right="12"/>
    </border>
    <children>
      <grid id="c112f" binding="autoUpdateConfigPanel" layout-manager="GridLayoutManager" row-count="3" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="12" bottom="12" right="12"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties/>
        <border type="etched" title="Auto Upgrade"/>
        <children>
          <component id="a66bf" class="javax.swing.JButton" binding="checkNowButton" default-binding="true">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="&amp;Check Now"/>
            </properties>
          </component>
          <grid id="5c68" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="8" fill="2" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="38314" class="javax.swing.JCheckBox" binding="chkUnstableVersionsCheckBox" default-binding="true">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <enabled value="false"/>
                  <text value="Check Snapshot Versions"/>
                </properties>
              </component>
              <hspacer id="dadcc">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false">
                    <minimum-size width="12" height="-1"/>
                  </grid>
                </constraints>
              </hspacer>
            </children>
          </grid>
          <component id="f1a3a" class="javax.swing.JCheckBox" binding="chkAutoUpdateEnabled">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="&amp;Enabled (Stable Version)"/>
            </properties>
          </component>
          <hspacer id="9bcf5">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
          <component id="90c98" class="javax.swing.JRadioButton" binding="checkNewVersionStable">
            <constraints>
              <grid row="1" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <selected value="true"/>
              <text value="Stable Only"/>
            </properties>
          </component>
          <component id="70258" class="javax.swing.JRadioButton" binding="checkNewVersionAll">
            <constraints>
              <grid row="2" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Stable + Snapshot"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="44730" class="javax.swing.JCheckBox" binding="reportAnonymousUsageStatisticsCheckBox" default-binding="true">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <forms/>
        </constraints>
        <properties>
          <enabled value="true"/>
          <selected value="false"/>
          <text value="&amp;Report anonymous usage statistics to help us develop a better plugin"/>
        </properties>
      </component>
      <grid id="8caa7" binding="httpProxyPanel" layout-manager="GridLayoutManager" row-count="4" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="12" bottom="12" right="0"/>
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="1" fill="1" indent="0" use-parent-layout="false"/>
          <forms defaultalign-vert="false"/>
        </constraints>
        <properties/>
        <border type="etched" title="HTTP Proxy"/>
        <children>
          <component id="2063e" class="javax.swing.JRadioButton" binding="chkUseIdeaProxy">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Use IDEA Proxy Settings"/>
            </properties>
          </component>
          <component id="d216f" class="javax.swing.JButton" binding="httpProxyButton">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="9" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Edit IDEA Proxy Settings"/>
            </properties>
          </component>
          <component id="10322" class="javax.swing.JRadioButton" binding="chkNoProxy">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Do Not Use Proxy"/>
            </properties>
          </component>
          <vspacer id="b998b">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
            </constraints>
          </vspacer>
          <component id="e9e70" class="javax.swing.JLabel">
            <constraints>
              <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="10"/>
              <text value="Information: You have to restart IDEA to apply changes in the proxy configuration"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
  <buttonGroups>
    <group name="checkNow">
      <member id="90c98"/>
      <member id="70258"/>
    </group>
    <group name="proxyButtonGroup">
      <member id="10322"/>
      <member id="2063e"/>
    </group>
  </buttonGroups>
</form>
