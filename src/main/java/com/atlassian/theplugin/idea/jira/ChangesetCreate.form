<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.atlassian.theplugin.idea.jira.ChangesetCreate">
  <grid id="cbd77" binding="contentPane" layout-manager="GridBagLayout">
    <constraints>
      <xy x="48" y="54" width="500" height="154"/>
    </constraints>
    <properties>
      <maximumSize width="-1" height="-1"/>
      <minimumSize width="-1" height="-1"/>
      <opaque value="false"/>
      <preferredSize width="500" height="200"/>
    </properties>
    <border type="empty">
      <size top="12" left="12" bottom="12" right="12"/>
    </border>
    <children>
      <component id="22087" class="javax.swing.JTextField" binding="changesetName">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
            <minimum-size width="150" height="-1"/>
            <preferred-size width="150" height="-1"/>
          </grid>
          <gridbag weightx="1.0" weighty="0.0"/>
        </constraints>
        <properties/>
      </component>
      <scrollpane id="d8d17">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <gridbag top="12" left="0" bottom="12" right="0" weightx="1.0" weighty="1.0"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="7ef5f" class="javax.swing.JTextArea" binding="changesetComment">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
      <component id="aed44" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Name:"/>
        </properties>
      </component>
      <component id="20f9f" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag top="0" left="0" bottom="0" right="12" weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Comment:"/>
        </properties>
      </component>
      <component id="9d016" class="javax.swing.JCheckBox" binding="isActive">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <selected value="false"/>
          <text value="Make this changelist active"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
